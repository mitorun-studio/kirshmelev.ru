<!doctype html><!-- Сайт https://mitosite/ -->
<html lang="{{ _site.siteLang or 'ru' }}" prefix="og: https://ogp.me/ns#">
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta charset="utf-8">
	<title>
		{% if title %}
			{{ title if page.url == "{{ '/' | url }}" else title + ' – ' + _site.siteName }}
		{% else %}
			{{ _site.siteName }}
		{% endif %}
	</title>
	<meta name="description" content="{{ description or _site.siteDescription }}">
	{% if keywords %}
		<meta name="keywords" content="{{ keywords }}">
	{% endif %}
	<meta name="author" content="mitorun.studio">
	<meta name="generator" content="Eleventy">
	<link rel="stylesheet" href="{{ '/styles.css' | url }}">
	<link rel="icon" sizes="any" href="{{ '/favicon.ico' | url }}">
	<link rel="icon" type="image/svg+xml" href="{{ '/img/favicon.svg' | url }}">
	<link rel="apple-touch-icon" sizes="180x180" href="{{ '/img/apple-touch-icon.png' | url }}">
	<link rel="manifest" href="{{ '/manifest.json' | url }}">
	{% if _site.siteThemeColor %}
		<meta name="theme-color" content="{{ _site.siteThemeColor }}">
	{% endif %}
	<link rel="preload" as="image" href="{{ '/img/sprite.svg' | url }}">
	<!--<link rel="preload" as="font" type="font/woff2" href="/fls/inter-100-900-cyrillic.woff2" crossorigin> <link rel="preload" as="font" type="font/woff2" href="/fls/inter-100-900-latin.woff2" crossorigin>-->
	<!-- htmlmin:ignore -->
	<!-- Yandex.Metrika counter -->
	<!--<script type="text/javascript"></script>-->
	<!-- /Yandex.Metrika counter -->
	<!-- htmlmin:ignore -->
	<meta property="og:type" content="website">
	<meta property="og:title" content="{{ title or _site.siteName }}">
	<meta property="og:description" content="{{ description or _site.siteDescription }}">
	<meta property="og:site_name" content="{{ _site.siteName }}">
	<meta property="og:locale" content="{{ _site.siteLang or 'ru' }}">
	{# <meta property="og:image" content="{{ _site.siteUrl }}/img/bg.jpg"> #}
	<meta property="og:image" content="https://v1.screenshot.11ty.dev/{{ _site.siteUrl + page.url }}/opengraph/bigger/">
	<!--<meta property="og:image:alt" content="Логотип и название бренда">-->
	<meta property="og:url" content="{{ _site.siteUrl + page.url }}">
	<link rel="canonical" href="{{ _site.siteUrl + page.url }}">
</head>

<body class="page-{% if pageClass %}{{ pageClass }}{% elif page.fileSlug %}{{ page.fileSlug }}{% else %}home{% endif %}">

	{% include "_header.njk" %}

	<main class="main" id="main" tabindex="-1">

		{{ content | safe }}

	</main>

	{% include "_footer.njk" %}




	<!-- Скрипт компонентов Bootstrap: -->
	<script defer="defer" src="{{ '/bootstrap.bundle.min.js' | url }}"></script>
	<script defer="defer" src="{{ '/fslightbox.js' | url }}"></script>

	<!-- Yandex.Metrika counter -->
	<!--<noscript></noscript>-->
	<!-- /Yandex.Metrika counter -->


	<noscript class="noscript"> У вас в браузере отключён JavaScript</noscript>

</body>
</html>
